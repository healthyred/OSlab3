  p = (char *) vm_extend();
  p[0] = 'e';
  p = (char *) vm_extend();
  p[0] = 'l';
  p = (char *) vm_extend();
  p[0] = 'l';
  p = (char *) vm_extend();
  p[0] = 'o';
  p = (char *) vm_extend();
  p[0] = '!';
